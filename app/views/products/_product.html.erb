<div id="<%= dom_id product %>">
  <p>
    <strong>Mechine:</strong>
    <%= product.mechine_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Selection:</strong>
    <%= product.selection %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= product.quantity %>
  </p>

</div>
